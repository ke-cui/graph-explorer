    "eslint-plugin-react-hooks": "4.5.0",
    "html-webpack-plugin": "5.5.0",
    "node-polyfill-webpack-plugin": "1.1.4",
    "prettier": "2.6.2",
    "raw-loader": "4.0.2",
    "rdf-parser-jsonld": "1.1.0",
    "rdf-parser-n3": "1.1.1",
